<h1>Listing Districts</h1>

<table class="table">
  <thead>
    <tr>
      <th>Source</th>
      <th>Country</th>
      <th>Region</th>
      <th>District</th>
      <th>Shapefile</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.source_id %></td>
        <td><%= district.country %></td>
        <td><%= district.region %></td>
        <td><%= district.district %></td>
        <td><%= district.shapefile %></td>
        <td><%= district.latitude %></td>
        <td><%= district.longitude %></td>
        <td><%= link_to 'Show', district %></td>
        <td><%= link_to 'Edit', edit_district_path(district) %></td>
        <td><%= link_to 'Destroy', district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New District', new_district_path %>
